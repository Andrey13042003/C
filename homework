#include <stdio.h>
#include <math.h>
int main(void){
    int a, b, c, d;
    float x1, x2;
    printf("Введите числа: a, b, c\n");
    scanf("%d %d %d", &a, &b, &c);
    if (a == 0)
    {
        printf("Уравнение не будет квадратным при a = 0, введите другие числа.");
    }
    else
    {
        d = pow(b, 2) - (4 * a * c);
        if (d > 0)
        {
            x1 = (-b + sqrt(d)) / (2 * a);
            x2 = (-b - sqrt(d)) / (2 * a);
            printf("x1 = %f x2 = %f", x1, x2);
        }
        else if (d == 0)
        {
            x1 = -b / (2 * a);
            printf("x1 = %f", x1);
        }
    }
    return 0;
}
